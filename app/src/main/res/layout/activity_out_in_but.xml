<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home.OutInButActivity">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/toolbar_layout"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="#607d8b"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/back_icon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="6dp"
            android:src="@drawable/ic_back"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="采购入库-李超"
            android:textSize="15sp"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>


        <View
            android:layout_width="1dp"
            android:layout_height="19dp"
            android:background="@color/white"
            android:layout_marginStart="250dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <TextView

            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:text="创建"
            android:gravity="center"
            android:textSize="15sp"
            android:textColor="@color/white"
            android:layout_marginStart="272dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <FrameLayout
        android:id="@+id/fl_search"
        android:layout_width="301dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="10dp"
        android:background="@drawable/search_rounded_rectangle"
        android:elevation="5dp"
        android:paddingStart="12dp"
        android:paddingTop="7dp"
        android:paddingBottom="7dp"
        >
        <EditText
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:hint="通过编号/主题关键词"
            android:textSize="13dp"
            android:background="@null"
            />
        <ImageView
            android:layout_width="1dp"
            android:layout_height="22dp"
            android:padding="1dp"
            android:layout_marginStart="256dp"
            android:layout_gravity="center_vertical"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:padding="1dp"
            android:layout_gravity="center_vertical"
            android:src="@drawable/outin1"
            android:layout_marginStart="267dp"
            />
    </FrameLayout>
    <com.xk.porject.ui.custom.XTextView
        android:id="@+id/tv_in"
        android:layout_width="97dp"
        android:layout_height="33dp"
        android:layout_marginStart="123dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="45dp"
        android:text="途中待入库"
        android:gravity="center"
        android:background="#245c06"
        android:textColor="@color/white"
        android:textSize="13dp"
        />
    <com.xk.porject.ui.custom.XTextView
        android:id="@+id/tv_out"
        android:layout_width="97dp"
        android:layout_height="33dp"
        android:layout_marginStart="226dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="45dp"
        android:text="途中待入库"
        android:gravity="center"
        android:background="#1e98d7"
        android:textColor="@color/white"
        android:textSize="13dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="材料名称"
        android:textColor="#333333"
        android:textSize="13dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="35dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:layout_marginTop="100dp"
        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/ThemeOverlay.Material3.AutoCompleteTextView.FilledBox"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="93dp"
        android:layout_marginTop="100dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="材料规格"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="35dp"
        android:textSize="13dp"
        android:textColor="#333333"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:layout_marginTop="144dp"
        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/ThemeOverlay.Material3.AutoCompleteTextView.FilledBox"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="93dp"
        android:layout_marginTop="144dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="材料型号"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="35dp"
        android:textColor="#333333"
        android:textSize="13dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:layout_marginTop="183dp"
        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/ThemeOverlay.Material3.AutoCompleteTextView.FilledBox"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="93dp"
        android:layout_marginTop="183dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="采购数量"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="35dp"
        android:textColor="#333333"
        android:textSize="13dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:layout_marginTop="222dp"
        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/ThemeOverlay.Material3.AutoCompleteTextView.FilledBox"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="93dp"
        android:layout_marginTop="222dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="单价"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="35dp"
        android:textColor="#333333"
        android:textSize="13dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:layout_marginTop="266dp"
        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/ThemeOverlay.Material3.AutoCompleteTextView.FilledBox"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="93dp"
        android:layout_marginTop="266dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="入库数量"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="35dp"
        android:textColor="#333333"
        android:textSize="13dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:layout_marginTop="305dp"
        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/ThemeOverlay.Material3.AutoCompleteTextView.FilledBox"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="93dp"
        android:layout_marginTop="305dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        android:background="@drawable/edittext_border_black"
        />
    <com.xk.porject.ui.custom.XTextView
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:text="确认"
        android:textSize="13dp"
        android:textColor="@color/white"
        android:gravity="center"
        app:cornerRadius = "5dp"
        android:background="#1e98d7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="370dp"
        app:layout_constraintTop_toBottomOf="@+id/fl_search"
        />
</androidx.constraintlayout.widget.ConstraintLayout>