<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home.RetrunOrderActivity">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/toolbar_layout"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="#607d8b"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/back_icon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="6dp"
            android:src="@drawable/ic_back"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="退库-李超"
            android:textSize="15sp"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>


        <View
            android:layout_width="1dp"
            android:layout_height="19dp"
            android:background="@color/white"
            android:layout_marginStart="250dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <TextView

            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:text="创建"
            android:gravity="center"
            android:textSize="15sp"
            android:textColor="@color/white"
            android:layout_marginStart="272dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <FrameLayout
        android:id="@+id/fl_search"
        android:layout_width="301dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="10dp"
        android:background="@drawable/search_rounded_rectangle"
        android:elevation="5dp"
        android:paddingStart="12dp"
        android:paddingTop="7dp"
        android:paddingBottom="7dp"
        >
        <EditText
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:hint="通过编号/主题关键词"
            android:textSize="13dp"
            android:background="@null"
            />
        <ImageView
            android:layout_width="1dp"
            android:layout_height="22dp"
            android:padding="1dp"
            android:layout_marginStart="256dp"
            android:layout_gravity="center_vertical"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:padding="1dp"
            android:layout_gravity="center_vertical"
            android:src="@drawable/outin1"
            android:layout_marginStart="267dp"
            />
    </FrameLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13dp"
        android:textColor=" #333333"
        android:text="选择编号"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="60dp"
        android:layout_marginStart="13dp"
        />
    <androidx.appcompat.widget.AppCompatSpinner
        android:layout_width="241dp"
        android:layout_height="25dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="55dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13dp"
        android:textColor=" #333333"
        android:text="材料名称"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="90dp"
        android:layout_marginStart="13dp"
        />
    <EditText
        android:layout_width="241dp"
        android:layout_height="25dp"
        android:layout_marginStart="71dp"
        android:padding="2dp"
        android:layout_marginTop="90dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13dp"
        android:textColor=" #333333"
        android:text="材料规格"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="121dp"
        android:layout_marginStart="13dp"
        />
    <EditText
        android:layout_width="241dp"
        android:layout_height="25dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="121dp"
        android:padding="2dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13dp"
        android:textColor=" #333333"
        android:text="材料型号"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="151dp"
        android:layout_marginStart="13dp"

        />
    <EditText
        android:layout_width="241dp"
        android:layout_height="25dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="151dp"
        android:padding="2dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13dp"
        android:textColor=" #333333"
        android:text="退回数量"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="181dp"
        android:layout_marginStart="13dp"
        />
    <EditText
        android:layout_width="241dp"
        android:layout_height="25dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="181dp"
        android:padding="2dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13dp"
        android:textColor=" #333333"
        android:text="单价"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="211dp"
        android:layout_marginStart="13dp"
        />
    <EditText
        android:layout_width="241dp"
        android:layout_height="25dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="211dp"
        android:padding="2dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13dp"
        android:textColor=" #333333"
        android:text="合计金额"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="242dp"
        android:layout_marginStart="13dp"
        />
    <EditText
        android:layout_width="241dp"
        android:layout_height="25dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="242dp"
        android:padding="2dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13dp"
        android:textColor=" #333333"
        android:text="图纸数量"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="274dp"
        android:layout_marginStart="13dp"
        />
    <EditText
        android:layout_width="241dp"
        android:layout_height="25dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="274dp"
        android:padding="2dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/edittext_border_black"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13dp"
        android:textColor=" #333333"
        android:text="实际使用"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        android:layout_marginTop="304dp"
        android:layout_marginStart="13dp"
        />
    <EditText
        android:layout_width="241dp"
        android:layout_height="25dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="304dp"
        android:padding="2dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/edittext_border_black"
        />
    <com.xk.base.ui.XTextView
        android:id="@+id/tv_send"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:text="确认"
        android:textColor="@color/white"
        android:background="#1e98d7"
        android:layout_marginTop="369dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_layout"
        app:layout_constraintStart_toStartOf="parent"
        android:gravity="center"
        android:textSize="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        />
</androidx.constraintlayout.widget.ConstraintLayout>