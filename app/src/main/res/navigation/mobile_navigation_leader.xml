<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation3"
    app:startDestination="@+id/navigation_project_home">

    <fragment
        android:id="@+id/navigation_project_home"
        android:name="com.xk.porject.teamleader.ui.LeaderMainFragment"
        android:label="主页"
        tools:layout="@layout/fragment_manage" >
        <action
            android:id="@+id/action_home_leader"
            app:destination="@id/navigation_persionner" />
    </fragment>

    <fragment
        android:id="@+id/navigation_persionner"
        android:name="com.xk.porject.teamleader.ui.LearderManagerFragment"
        android:label="管理"
        tools:layout="@layout/activity_contractor_leader" />

    <fragment
        android:id="@+id/navigation_project_manage"
        android:name="com.xk.porject.teamleader.ui.LearderManagerFragment"
        android:label="管理"
        tools:layout="@layout/activity_contractor_leader" />

    <fragment
        android:id="@+id/navigation_project_notifications"
        android:name="com.xk.porject.home.fragment.NotificationsFragment"
        android:label="消息"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_project_money"
        android:name="com.xk.porject.home.fragment.MoneyFragment"
        android:label="工资"
        tools:layout="@layout/fragment_money" />
    <fragment
        android:id="@+id/navigation_project_settings"
        android:name="com.xk.porject.home.fragment.SettingsFragment"
        android:label="设置"
        tools:layout="@layout/fragment_settings" />
</navigation>