<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.xk.porject.home.fragment.HomeFragment"
        android:label="主页"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_manage"
        android:name="com.xk.porject.home.fragment.ProjectPersonnelFragment"
        android:label="管理"
        tools:layout="@layout/activity_cmactivity" >
    </fragment>
    <fragment
        android:id="@+id/PersonnelFragment"
        android:name="com.xk.porject.projectmain.ui.ProjectManageFragment"
        />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.xk.porject.home.fragment.NotificationsFragment"
        android:label="消息"
        tools:layout="@layout/activity_cmactivity" />
    <fragment
        android:id="@+id/navigation_money"
        android:name="com.xk.porject.home.fragment.MoneyFragment"
        android:label="资金"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.xk.porject.home.fragment.SettingsFragment"
        android:label="设置"
        tools:layout="@layout/fragment_notifications" />
</navigation>